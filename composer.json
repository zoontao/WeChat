{
    "name": "zoontao/wechat",
    "type": "library",
    "description": "WeChat API SDK for Lavarel",
    "keywords": ["wechat", "weixin","laravel", "sdk","zoontao"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bell",
            "email": "bell@zoontao.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "easywechat-composer/easywechat-composer": "^1.1",
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog": "^1.22 || ^2.0",
        "overtrue/socialite": "~2.0",
        "pimple/pimple": "^3.0",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^3.3 || ^4.3 || ^5.0",
        "symfony/event-dispatcher": "^4.3 || ^5.0",
        "symfony/http-foundation": "^4.3",
        "symfony/psr-http-message-bridge": "^0.3 || ^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zoontao\\WeChat\\ServiceProvider"
            ],
            "aliases": {
                "WeChat": "Zoontao\\WeChat\\Facade"
            }
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.2.3",
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Zoontao\\WeChat\\": "src/"
        }
    }
}
